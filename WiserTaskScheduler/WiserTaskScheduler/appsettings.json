{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "C:/temp/wiser_task_scheduler/log.txt",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "Console"
      }
    ],
    "Enrich": [
      "FromLogContext"
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Wts": {
    "Name": "WTS development",
    "SecretsBaseDirectory": "C:\\WiserSecrets\\",
    "MainService": {
      "LocalConfiguration": "C:\\WTS configurations\\WTSTestOAuth.xml",
      "RunScheme": {
        "Type": "Continuous",
        "TimeId": 0,
        "Delay": "00:05:00"
      }
    },
    "CleanupService": {
      "FileFolderPaths": [ "C:/temp/wiser_task_scheduler/" ]
    }
  }
}
